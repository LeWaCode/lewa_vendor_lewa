<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:id="@+id/battery_calibration_message_linearlayout"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:orientation="vertical">
  	<TextView android:id="@+id/battery_calibration_message_txt"
  		android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:text="@string/spm_battery_calibration_notification_message"
  		android:textSize="20dip"/>
  	<TextView android:id="@+id/battery_calibration_message_define"
  		android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:text="@string/spm_battery_calibration_notification_message_define"
  		android:textSize="15dip"/>
</LinearLayout>
